{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "ES2020"
    ],
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "obsidian"
    ],
    "baseUrl": ".",
    "paths": {
      "templater": [
        "types/templater.d.ts"
      ]
    }
  },
  "include": [
    "**/*.ts"
  ]
}
