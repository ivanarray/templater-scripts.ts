{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["ES2020"],
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["obsidian"],
    "baseUrl": ".",
    "paths": {
      "templater": ["types/templater.d.ts"]
    },
    "removeComments": false
  },
  "include": ["**/*.ts"]
}
